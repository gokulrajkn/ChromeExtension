{
  "name": "To Do List",
  "version": "0.0.0.1",
    "author":"kngokulrajkn@gmail.com",
  "description": "An advanced To Do List with no sign up required",
  "manifest_version": 2,
"content_security_policy":"script-src 'self' 'unsafe-inline' 'sha256-2DfzJ6Lhduqq1W8Yh+UhnYz5Jx9tu9iybye+mRl9kHM='",
"permissions": ["activeTab","declarativeContent","storage","http://*/*", 
  "https://*/*","tabs"],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "icons": {
       "16": "images/123.png",
      "32": "images/123.png",
      "48": "images/123.png",
      "128": "images/123.png"
    },
"browser_action": {
      "default_popup": "popup.html",
"default_title": "To Do List", 
 "default_icon": {
        "16": "images/123.png",
      "32": "images/123.png",
      "48": "images/123.png",
      "128": "images/123.png"
      }
    }
}
